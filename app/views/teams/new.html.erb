<%= form_for @team do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
    <%= render partial: "error_for_field", locals: { model: @team, field: :name } %>

  <%= f.label :project_id %>
  <%= f.select(:project_id, Project.all.collect {|proj| [ proj.name, proj.id ] }, {:include_blank => "" } ) %>
  
  <%= f.submit "Create Team" %>
<% end %>

